
<table class="table">
    <tr>
        <th>Name</th>
        <th>Action</th>
    </tr>

    @foreach (var item in Model.Items)
    {
        <tr>
            <td>@item.Name</td>
            <td><a asp-controller="@Model.Controller" asp-action="Edit" asp-route-id="@item.ID">Edit</a></td>
        </tr>
        
    }
</table> 